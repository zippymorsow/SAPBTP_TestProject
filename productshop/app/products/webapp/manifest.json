{
  "_version": "1.81.0",
  "sap.app": {
    "id": "ns.products",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.1",
      "toolsId": "33466145-addc-4e83-a79f-2d17cafc7fcf"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/productshop/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.144.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ns.products.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "ProductList",
          "target": "ProductList"
        },
        {
          "pattern": "Product({key}):?query:",
          "name": "ProductObjectPage",
          "target": "ProductObjectPage"
        }
      ],
      "targets": {
        "ProductList": {
          "type": "Component",
          "id": "ProductList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Product",
              "variantManagement": "Page",
              "navigation": {
                "Product": {
                  "detail": {
                    "route": "ProductObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              },
              "initialLoad": "Enabled"
            }
          }
        },
        "ProductObjectPage": {
          "type": "Component",
          "id": "ProductObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": true,
              "contextPath": "/Product",
              "content": {
                "header": {
                  "anchorBarVisible": true
                },
                "footer": {
                  "actions": {
                    "12345": {
                      "press": "ns.products.ext.controller.Anything.Sampler",
                      "visible": true,
                      "enabled": true,
                      "text": "Custom Action ID",
                      "position": {
                        "placement": "After",
                        "anchor": "DataFieldForAction::productshop.orderProduct"
                      }
                    }
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.FieldGroup#formsection": {
                  "fields": {
                    "FN": {
                      "template": "ns.products.ext.fragment.FN",
                      "label": "sample field"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.fe": {
    "app": {
      "enableLazyLoading": true
    }
  }
}
